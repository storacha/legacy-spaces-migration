# Environment Configuration
# Set to 'staging' or 'production' (default: production)
# This automatically configures table names, regions, and service URLs
STORACHA_ENV=production

# AWS Credentials
# Same credentials work for both staging and production
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# AWS Region (optional - auto-configured based on STORACHA_ENV)
# production: us-west-2, staging: us-east-2
# AWS_REGION=us-west-2

# DynamoDB Tables (optional - auto-configured based on STORACHA_ENV)
# Uncomment to override defaults
# UPLOAD_TABLE_NAME=prod-w3infra-upload
# STORE_TABLE_NAME=prod-w3infra-store
# BLOB_REGISTRY_TABLE_NAME=prod-w3infra-blob-registry
# ALLOCATION_TABLE_NAME=prod-w3infra-allocation
# CONSUMER_TABLE_NAME=prod-w3infra-consumer

# Services (optional - auto-configured based on STORACHA_ENV)
# Uncomment to override defaults
# INDEXING_SERVICE_URL=https://indexer.storacha.network
# CONTENT_CLAIMS_SERVICE_URL=https://claims.web3.storage
# UPLOAD_SERVICE_URL=https://up.storacha.network

# R2/S3 Storage (optional - auto-configured based on STORACHA_ENV)
# Uncomment to override defaults
# CARPARK_BUCKET=carpark-prod-0
# CARPARK_PUBLIC_URL=https://carpark-prod-0.r2.w3s.link
# CLAIM_SERVICE_BUCKET=prod-storage-claim-store-bucket

# Migration Settings
BATCH_SIZE=100
MAX_CONCURRENCY=10
DRY_RUN=true

# Service Credentials (for publishing claims and acting on behalf of spaces)
# Get these from the upload service team
SERVICE_PRIVATE_KEY=your_base64_encoded_ed25519_private_key
SERVICE_DID=did:web:up.storacha.network

# Admin Key (for ucan/attest)
# ADMIN_PRIVATE_KEY=...
