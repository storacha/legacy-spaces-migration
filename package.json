{
  "name": "legacy-spaces-migration",
  "version": "0.1.0",
  "type": "module",
  "description": "Migration tooling for legacy spaces to modern indexing system",
  "scripts": {
    "migrate": "node src/migrate.js",
    "debug:index": "node scripts/debug-index-claim.js",
    "estimate": "node src/estimate-costs.js",
